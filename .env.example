# AWS Infrastructure Environment Variables
# Copy this file to .env and update with your actual values

# Frontend Instance (Public IP from Terraform output)
FRONTEND_IP=YOUR_FRONTEND_PUBLIC_IP

# Backend Instance (Private IP from Terraform output)
BACKEND_IP=YOUR_BACKEND_PRIVATE_IP

# Database Instance (Private IP from Terraform output)
DB_IP=YOUR_DATABASE_PRIVATE_IP

# AWS Configuration
AWS_REGION=ap-northeast-2
AWS_PROFILE=default

# SSH Configuration
SSH_KEY_PATH=~/.ssh/your-key-pair.pem

# Application Configuration
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_secure_password
POSTGRES_DB=postgres

# Monitoring Configuration
GRAFANA_ADMIN_PASSWORD=your_grafana_password
