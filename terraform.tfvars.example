# Terraform Variables - Example Template
# Copy this file to terraform.tfvars and fill in your actual values
# terraform.tfvars is gitignored and will not be committed

# AWS Region where infrastructure will be deployed
aws_region = "ap-northeast-2"

# Name of your SSH key pair in AWS (must exist in the region above)
key_pair_name = "your-key-pair-name"

# Your public IP address for SSH access (get it with: curl ifconfig.me)
# Format: x.x.x.x/32 (the /32 means exactly this IP)
my_ip = "0.0.0.0/32"
